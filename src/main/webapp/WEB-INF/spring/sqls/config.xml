		<typeAlias alias="Coupon_historyDto" type="kh.coupon.mvc.dto.Coupon_historyDto"/>
	</typeAliases>
		
	<mappers>
		<mapper resource="/sqls/home_mapper.xml"/>
		<mapper resource="/sqls/member_mapper.xml"/>
		<mapper resource="/sqls/user_mapper.xml"/>
		<mapper resource="/sqls/academy_mapper.xml"/>
		<mapper resource="/sqls/client_mapper.xml"/>
		<mapper resource="/sqls/coupon_mapper.xml"/>
		<mapper resource="/sqls/review_mapper.xml"/>
		<mapper resource="/sqls/expense_mapper.xml"/>
	</mappers>
</configuration>
