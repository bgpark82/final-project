<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<!-- 각 Dto들을 잡아주면된다. -->
	<typeAliases>
		<typeAlias alias="MemberDto" type="kh.coupon.mvc.dto.MemberDto"/>
		<typeAlias alias="CouponDto" type="kh.coupon.mvc.dto.CouponDto"/>
		<typeAlias alias="ClientDto" type="kh.coupon.mvc.dto.ClientDto"/>
